@incollection{moini2022a,
  title = {Pathogenesis of {{Cancer}}},
  booktitle = {Global {{Epidemiology}} of {{Cancer}}},
  author = {Moini, Jahangir},
  date = {2022},
  pages = {1--22},
  publisher = {{John Wiley \& Sons, Ltd}},
  doi = {10.1002/9781119817192.ch1},
  url = {http://onlinelibrary.wiley.com/doi/abs/10.1002/9781119817192.ch1},
  urldate = {2022-11-28},
  abstract = {Cancer has been the primary cause of death throughout the world for thousands of years. There are some forms of cancer that can be treated successfully, especially when diagnosed early in their development. Other cancers are far less treatable. Understanding the pathogenesis of cancer helps us to find better and more focused treatments, and even to find preventive measures against the disease. There are actually more than 100 different types of cancer, but all of them arise from just one cell that experiences a genetic alteration, and then defies the rules of normal cell division. Cancer cells do not respond to external signals concerning their division, differentiation into other cells, and death. This causes an overgrowth of tissues that are commonly known as tumors , which may be benign or malignant. The process of migration of malignant tumor cells is known as metastasis. The term “neoplasia” refers to new growth, which can be normal or abnormal. Tumor cells generally have shapes that appear abnormal in comparison with other cells, and their biochemical reactions are not as well controlled. Some tumors make hormones that cause dysregulated cellular function known as anaplasia.},
  isbn = {978-1-119-81719-2},
  langid = {english},
  keywords = {Aneuploidy,cellular plasticity,chromatin,double minutes,epigenetic,gene rearrangements,intravasate,metastasis,penetrance,perforin},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781119817192.ch1},
  file = {E\:\\OneDrive - DePaul University\\LITERATURE\\Zotero\\storage\\GNS7LB29\\2022 - Pathogenesis of Cancer.pdf;E\:\\OneDrive - DePaul University\\LITERATURE\\Zotero\\storage\\BY9WJAHY\\9781119817192.html}
}


